{
    "name": "Vision Voice",
    "version": "1.0.0",
    "description": "First Version For Vision Voice",
    "manifest_version": 2,
    "permissions": [
      "storage",
      "activeTab",
      "tts",
      "fileSystem"
    ],
    "web_accessible_resources": [
      "Text_com/mathplanet_links.txt"
    ],
    "background": {
      "persistent": true,
      "scripts": ["background.js"]
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  